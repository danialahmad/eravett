<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering 
  SYSTEM "http://www.hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >
<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="INTEGER" hibernate-type="java.lang.Integer" />
		<sql-type jdbc-type="NUMERIC" hibernate-type="java.lang.Integer" />
		<sql-type jdbc-type="CHAR" hibernate-type="java.lang.String" />
		<sql-type jdbc-type="OTHER" hibernate-type="java.sql.Timestamp" />
	</type-mapping>

	<table-filter match-schema="public" match-name=".*"
		exclude="true" />
	<table-filter match-schema="uam" match-name=".*" exclude="false" />
	<table-filter match-schema="eravett" match-name=".*"
		exclude="false" />

	<table schema="eravett" name="profile">
		<primary-key>
			<generator class="identity" />
		</primary-key>
	</table>

	<table schema="eravett" name="address">
		<primary-key>
			<generator class="identity" />
		</primary-key>
	</table>

	<table schema="eravett" name="file_blob">
		<primary-key>
			<generator class="identity" />
		</primary-key>
	</table>
	
	<table schema="eravett" name="agency">
		<primary-key>
			<generator class="identity" />
		</primary-key>
	</table>

	<table schema="eravett" name="asset_owner">
		<primary-key>
			<generator class="identity" />
		</primary-key>
	</table>
	
	<table schema="eravett" name="vetting">
		<primary-key>
			<generator class="identity" />
		</primary-key>
	</table>
	
	<table schema="eravett" name="vetting_order">
		<primary-key>
			<generator class="identity" />
		</primary-key>
	</table>
	
	<table schema="eravett" name="vetting_profile">
		<primary-key>
			<generator class="identity" />
		</primary-key>
	</table>
	
	<table schema="eravett" name="vetting_status">
		<primary-key>
			<generator class="identity" />
		</primary-key>
	</table>

</hibernate-reverse-engineering>