			<groupId>org.webjars</groupId>
			<artifactId>raphaeljs</artifactId>
			<version>2.1.4</version>
			<scope>runtime</scope>
		</dependency>
		<dependency>
			<groupId>org.webjars.bower</groupId>
			<artifactId>twbs-pagination</artifactId>
			<version>1.2.5</version>
			<scope>runtime</scope>
		</dependency>
